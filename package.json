{
  "name": "articon2-backend",
  "version": "1.0.0",
  "description": "Backend API for Articon Hackathon",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec tsx --require dotenv/config src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "setup-db": "tsx scripts/setup-database.ts",
    "create-admin": "tsx scripts/create-admin.ts",
    "setup-tasks": "tsx scripts/setup-tasks.ts",
    "temp": "tsx scripts/add-score-column.ts"
  },
  "keywords": [
    "hackathon",
    "express",
    "typescript",
    "supabase"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-sesv2": "^3.927.0",
    "@hookform/resolvers": "^5.2.2",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^11.0.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-validator": "^7.3.0",
    "framer-motion": "^12.23.24",
    "google-auth-library": "^10.5.0",
    "googleapis": "^164.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.552.0",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.10",
    "react-hook-form": "^7.66.0",
    "socket.io": "^4.8.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.79.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.3",
    "nodemon": "^3.1.10",
    "supabase": "^2.54.11",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
