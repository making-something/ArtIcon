{
  "name": "articon-backend",
  "version": "1.0.0",
  "description": "Backend API for Articon Hackathon",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec tsx --require dotenv/config src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node -r dotenv/config dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "setup-db": "tsx scripts/setup-database.ts",
    "create-admin": "tsx scripts/create-admin.ts",
    "setup-tasks": "tsx scripts/setup-tasks.ts"
  },
  "keywords": [
    "hackathon",
    "express",
    "typescript",
    "sqlite"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-ses": "^3.936.0",
    "@aws-sdk/client-sesv2": "^3.927.0",
    "@types/axios": "^0.14.4",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.4.5",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-validator": "^7.3.0",
    "google-auth-library": "^10.5.0",
    "googleapis": "^164.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.10",
    "socket.io": "^4.8.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.3",
    "eslint": "^9.39.1",
    "nodemon": "^3.1.10",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
